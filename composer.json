{
  "name": "petrknap/petrknap.cz",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Petr Knap",
      "email": "dev@petrknap.cz"
    }
  ],
  "config": {
    "platform": {
      "php": "7.0",
      "ext-dom": true,
      "ext-curl": true
    }
  },
  "require": {
    "netpromotion/profiler": "1.4.0-rc1 as 1.4",
    "petrknap/nette-bootstrap": "^1.2",
    "nette/application": "^2.4",
    "nette/caching": "^2.5",
    "tracy/tracy": "^2.4",
    "curl/curl": "^1.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "dg/ftp-deployment": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "PetrKnap\\Web\\": "app/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PetrKnap\\Web\\Test\\": "tests/"
    }
  },
  "scripts": {
    "tests/": [
      "make tests-run"
    ],
    "post-autoload-dump": [
      "@tests/"
    ]
  }
}
